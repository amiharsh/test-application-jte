pipeline {
    parameters {
        string(name: 'BRANCH', defaultValue: 'main', description: 'Enter the branch or tag to deploy')
    }
  stages {
    stage('Source Control From Application') {
      steps {
        checkout_branch(params.GIT_BRANCH)
      }
    }
  }
  post {
    success {
      echo 'Build succeeded'
    }
    failure {
      echo 'Build failed'
    }
  }
}
